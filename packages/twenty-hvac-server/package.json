{
  "name": "twenty-hvac-server",
  "version": "0.1.0",
  "description": "HVAC Backend Server - Pasja rodzi profesjonalizm",
  "author": "Fulmark HVAC Team",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "nx": "NX_DEFAULT_PROJECT=twenty-hvac-server node ../../node_modules/nx/bin/nx.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "../../node_modules/typeorm/.bin/typeorm",
    "database:migrate": "npx typeorm migration:run -d src/config/database.config.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@graphql-yoga/nestjs": "^2.1.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@sentry/nestjs": "^8.55.0",
    "ai": "^4.3.16",
    "axios": "^1.6.0",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "helmet": "^7.1.0",
    "imapflow": "^1.0.189",
    "ioredis": "^5.3.2",
    "lodash.isobject": "^3.0.2",
    "lodash.merge": "^4.6.2",
    "lodash.omitby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "mailparser": "^3.7.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "pg": "^8.11.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "weaviate-ts-client": "^1.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash.isobject": "^3.0.7",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/lodash.uniq": "^4.5.9",
    "@types/lodash.uniqby": "^4.7.9",
    "@types/mailparser": "^3",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^22.12.0",
    "npm": "please-use-yarn",
    "yarn": "^4.0.2"
  }
}
